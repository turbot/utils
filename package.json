{
  "name": "@turbot/utils",
  "author": "Turbot HQ, Inc",
  "version": "5.1.0",
  "description": "Turbot Utils Library.",
  "license": "Apache-2.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/turbot/utils.git"
  },
  "scripts": {
